{
  "name": "gmsmith",
  "description": "GM engine for spritesmith",
  "version": "0.5.4",
  "homepage": "https://github.com/twolfson/gmsmith",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/gmsmith.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/gmsmith/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/twolfson/gmsmith/blob/master/LICENSE-MIT"
    }
  ],
  "main": "lib/gmsmith",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "lint": "twolfson-style lint lib/ test/",
    "precheck": "twolfson-style precheck lib/ test/",
    "preinstall": "gm -version || convert -version",
    "pretest": "twolfson-style install",
    "test": "npm run precheck && mocha --reporter dot --timeout 60000 && npm run lint"
  },
  "dependencies": {
    "async": "~0.2.7",
    "gm": "~1.17.0",
    "obj-extend": "~0.1.0",
    "temporary": "0.0.8",
    "which": "~1.0.5"
  },
  "devDependencies": {
    "foundry": "~4.3.2",
    "foundry-release-git": "~2.0.2",
    "foundry-release-npm": "~2.0.2",
    "jscs": "~2.4.0",
    "jshint": "~2.8.0",
    "mocha": "~1.21.4",
    "spritesmith-engine-test": "~2.0.0",
    "twolfson-style": "~1.6.1"
  },
  "keywords": [
    "phantomjs",
    "spritesmith",
    "image",
    "layout"
  ],
  "foundry": {
    "registerVersion": "0.1.0",
    "releaseCommands": [
      "foundry-release-git",
      "foundry-release-npm"
    ]
  }
}